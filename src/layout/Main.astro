---
const { title, description } = Astro.props
---

<html lang="en">
	<head>
		<title>{ title }</title>
		<meta charset="utf-8">
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width,initial-scale=1">
		<meta name="description" content={ description }>
		<meta name="generator" content={ Astro.generator } />
	</head>
	<body>
		<slot />
	</body>
</html>
